<!DOCTYPE codename-titlestate >
<titlestate>
	<sprites folder="menus/title">
		
		<sprite name="bg" sprite="bg" x="-700" y="-150" scale="0.75"/>
		<sprite name="cover" sprite="sidebar" x="228" y="-180" scale="0.67"/>
		<sprite name="logo" sprite="logo" x="740" y="50" scale="0.9" beatInterval="1"/>
		<sprite name="enter" sprite="enter" x="690" y="445" scale="0.67" beatInterval="1"/>
		

		
	</sprites>

	<intro length="16">
		<text beat="1">
			<line text="ninjamuffin99" />
			<line text="phantomArcade" />
			<line text="kawaisprite" />
			<line text="evilsk8r" />
		</text>

		<text beat="3">
			<line text="ninjamuffin99" />
			<line text="phantomArcade" />
			<line text="kawaisprite" />
			<line text="evilsk8r" />
			<line text="presents" />
		</text>

		<text beat="4" /> <!-- clears the screen !-->

		<text beat="5">
			<line text="In association" />
			<line text="with" />
		</text>

		<text beat="7">
			<line text="In association" />
			<line text="with" />
			<line text="Newgrounds" />
			<sprite path="menus/titlescreen/newgrounds_logo" scale="0.8"/>
		</text>

		<text beat="8" /> <!-- clears the screen !-->

		<text beat="9">
			<introtext line="1" />
		</text>

		<text beat="11">
			<introtext line="1" />
			<introtext line="2" />
		</text>

		<text beat="12"/>

		<text beat="13">
			<line text="Friday" />
		</text>

		<text beat="14">
			<line text="Friday" />
			<line text="Night" />
		</text>

		<text beat="15">
			<line text="Friday" />
			<line text="Night" />
			<line text="Funkin'" />
			<line text="Soft" />
		</text>
	</intro>
</titlestate>